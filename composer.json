{
  "name": "rlmp/rlmp-language-detection",
  "type": "typo3-cms-extension",
  "description": "This plugin detects the visitor's preferred language and sets the local configuration for TYPO3's language engine accordingly. Both, one-tree and multiple tree concepts, are supported. It can also select from a list of similar languages if the user's preferred language does not exist.",
  "homepage": "https://typo3.org/extensions/repository/view/rlmp_language_detection",
  "license": [
    "GPL-2.0+"
  ],
  "keywords": [
    "TYPO3",
    "extension",
    "language",
    "detection",
    "localization",
    "multilanguage"
  ],
  "authors": [
    {
      "name": "Thomas LÃ¶ffler",
      "email": "loeffler@spooner-web.de",
      "homepage": "https://spooner-web.de",
      "role": "Developer"
    }
  ],
  "version": "7.0.6",
  "require": {
    "typo3/cms": ">6.2",
    "typo3-ter/static-info-tables": ">6.3"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "htdocs"
    }
  },
  "autoload": {
    "psr-4": {
      "Rlmp\\RlmpLanguageDetection\\": "Classes"
    }
  }
}
